    var (_, _) = invoke_method(participate, [addresses.tuple_index(0), 15, 65536]);
    (cell addr2stake, cell stake2info) = get_participate_dicts(1);
    addr2stake.check_addr2stake(addresses.tuple_index(0), 15);
